{
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true,
		},
	},
	"parser": "babel-eslint",
	"env": {
		"es6": true,
		"browser": true,
		"node": true,
		"jquery": true,
		"mongo": true,
	},
	"plugins": [
		"react"
	],
	"rules": {
		"comma-dangle": 2,
		"no-cond-assign": 2,
		"no-console": 1,
		"no-constant-condition": 1,
		"no-dupe-keys": 1,
		"no-empty": 2,
		"no-empty-character-class": 2,
		"no-extra-boolean-cast": 1,
		"no-extra-semi": 1,
		"no-func-assign": 1,
		"no-inner-declarations": 1,
		"no-invalid-regexp": 2,
		"no-irregular-whitespace": 2,
		"no-unreachable": 2,
		"use-isnan": 2,
		"valid-typeof": 2,
		"no-implied-eval": 2,
		"curly": 2,
		"consistent-return": 2,
		"default-case": 2,
		"dot-notation": 2,
		"eqeqeq": 2,
		"guard-for-in": 2,
		"no-alert": 2,
		"no-undefined": 0,
		"no-unused-vars": 2,
		"no-use-before-define": 2,
		"no-mixed-requires": 2,
		"camelcase": 1,
		"comma-spacing": 2,
		"comma-style": 2,
		"eol-last": 2,
		"new-cap": 2,
		"no-mixed-spaces-and-tabs": 2,
		"no-multiple-empty-lines": 2,
		"semi-spacing": 2,
		"no-spaced-func": 2,
		"no-trailing-spaces": 1,
		"operator-assignment": 2,
		"quotes": [2, 'single'],
		"space-before-function-paren": 1,
		"keyword-spacing": 1,
		"space-before-blocks": 1,
		"object-curly-spacing": 1,
		"space-infix-ops": 1,
		"no-var": 2,
	}
}
